package com.core.java.collections;

import java.util.ArrayList;

public class ArrayListTest {

	public static void main(String[] args) {
		
		ArrayList myList = new ArrayList();
		
		myList.add(10);
		
		myList.add(20);
		
		myList.add(10);
		
		System.out.println(myList);
		
		myList.add("vinod");
		
		myList.add("rakesh");
		
		myList.add("java");
		
		System.out.println(myList);
	}
}

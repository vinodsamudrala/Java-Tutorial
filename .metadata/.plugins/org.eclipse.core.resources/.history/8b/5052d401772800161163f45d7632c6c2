package com.core.java.overriding;

class Parent {
	
	int salary = 100;
	
	public void method() {
		System.out.println("Inside Parent");
	}
}

class Child extends Parent {

	public void method() {
	
		System.out.println("Inside Child");
	
	}
}

public class OverringTest {
	
	Child chaild = new Child();
	
	

}